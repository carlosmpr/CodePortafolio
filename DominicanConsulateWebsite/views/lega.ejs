	<!-- Page Content------------------------------------------------ -->
	<aside class="page-content-wrapper">
<body>
    
 <div class=" card container panel shadow">
    <div class="jumbotron panel-primary">
        <h1><%=fPasaporte.name %></h1>
    </div>
    
        <div class="panel panel-primary">
  <div class="panel-heading"><p ><%=fPasaporte.Descripcion %></p></div>
  <div class="panel-heading"><p ><%=fPasaporte.observaciones %></p></div>
  
  
  
    
   
    </div>
    
    <div class="panel panel-warning">
        <div class="panel-heading"><p ><%=fPasaporte.other%></p></div>
        </div>
    <div class="row">
        <div class="col-xs-3" id="myScrollspy">
            <ul class="nav nav-tabs nav-stacked affix-top" data-spy="affix" data-offset-top="190" >
                <li class="active"><a href="#section-1">Requisitos</a></li>
                <li><a href="#section-2">Pasos</a></li>
                <li><a href="#section-3">Citas Online</a></li>
                <li><a href="#section-4">Descargar</a></li>
                <li><a href="#section-5">Tiempo y Costo</a></li>
                <li><a href="#section-6">contactanos</a></li>
            </ul>
        </div>
        <div class="col-xs-9">
            <h2 id="section-1">Documentos Requeridos</h2>
            <hr>
            
            
            <% for (var i = 0; i < fPasaporte.Requisitos.length; i++) { %>
            
               <h4><%=fPasaporte.Requisitos[i] %></h4>
            <img width="100%" src="<%=fPasaporte.RequisiImg[i] %>"/>
          
          
               <% }; %>
            
        
            <hr>
            <h2 id="section-2">Pasos para realizar el Servicio</h2>
            <img width="100%"src="<%=fPasaporte.Pasos %>"/>
            <hr>
            
            
            <h2 id="section-5">Tiempo y costo del servicio</h2>
          <ul>
              <h4>Tiempo del servicio: <strong><%=fPasaporte.tiempo%></strong></h4>
              <h4>Costo del servicio: <strong><%=fPasaporte.costo %></strong> </h4>
              </ul>
               <hr>
               
            <h2 id="section-3">Citas Online</h2>
            
            <h1>Video de Como Hacer citas</h1>
          <div class="embed-responsive embed-responsive-16by9">
  <iframe src="https://www.youtube.com/embed/29huXtqQhcA" width="560" height="316" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allowFullScreen="true"></iframe>
</div>
            
            <button type="button" class="btn btn-warning">Hacer mi cita online</button>
             <hr>
            <h2 id="section-4">Descargar Requisitos, Pasos y Formulario</h2>
         <a href="<%=fPasaporte.Pasos %>" download>Descargar Pasos para realizar la Renovacion de Pasaporte</a>
          <hr>
          
              <h2 id="section-6">Asistencia</h2>
                <h4>Si necesita asistencia para realizar este servicio puede comunicarse con nosotros de las siguiente manera:</h4>
            
          <ul>
              <li>Llamando a: 212-768-2480</li>
            <li>Correo:info@consuladord-ny.org</li>
            <li>Facebook</li>
            <li>Instagram</li>
            <li>twitter</li>
              </ul>
        </div>
    </div>
</div>

 </aside>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
